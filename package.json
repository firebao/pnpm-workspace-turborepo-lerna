{
  "name": "pnpm-workspace-turbo-repo-lerna",
  "version": "1.0.0",
  "description": "my local monorepo",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "commit": "commit",
    "prepare": "husky",
    "preinstall": "node ./scripts/preinstall.js",
    "test:unit": "vitest --watch=false",
    "lerna-init": "lerna init",
    "lerna-create": "lerna create",
    "lerna-publish": "lerna publish",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint"
  },
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.3",
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/prompt-cli": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.17",
    "chalk": "^5.4.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-turbo": "^2.5.4",
    "eslint-plugin-prettier": "^5.4.0",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.7",
    "lerna": "^8.2.2",
    "lint-staged": "^16.0.0",
    "lodash": "^4.17.21",
    "prettier": "3.5.3",
    "turbo": "^2.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "nx"
    ]
  },
  "engines": {
    "node": ">=20.12.2",
    "pnpm": ">=10.4.0"
  },
  "lint-staged": {
    "packages/**/src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "jsencrypt": "^3.5.4",
    "vue-i18n": "9.2.2"
  }
}
